import java.util.Scanner;

public class Main{
	public static void main(String [] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Quantas galinhas você possui?");
		int galinha = sc.nextInt();
		
		double anelD = 4.00;
		double anelE = 3.50;
		
		double totalGranja = (anelD + anelE) * galinha;
		
		System.out.println("O valor total da granja é de R$" + totalGranja);
		
		sc.close();
	}
} 
